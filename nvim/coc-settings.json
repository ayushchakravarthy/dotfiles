{
  "suggest.noselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
	"cpp",
	"cc",
	"python"
  ],
  "python.jediPath": "/usr/local/lib/python3.8/dist-packages",
  "languageserver": {
        "ccls": {
            "command": "ccls",
            "filetypes": [
                "c",
		"cc",
                "cpp",
                "objc",
                "objcpp"
            ],
            "rootPatterns": [
                ".ccls",
                "compile_commands.json",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "initializationOptions": {
                "cache": {
                    "directory": "/tmp/ccls"
                }
            }
        }
    }
}
